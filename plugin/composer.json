{
  "name": "wstrategies/wp-bracket-builder",
  "authors": [
    {
      "name": "Barry Molina",
      "email": "barry@wstrategies.co"
    }
  ],
  "autoload": {
    "psr-4": {
      "WStrategies\\BMB\\": ""
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*"
      ]
    },
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "version": "dev-main",
        "source": {
          "url": "https://github.com/WordPress/wordpress-develop",
          "type": "git",
          "reference": "origin/trunk"
        }
      }
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "yoast/phpunit-polyfills": "^1.0",
    "spatie/phpunit-snapshot-assertions": "^4.2",
    "wpackagist-plugin/woocommerce": "8.4.0",
    "wordpress/wordpress": "dev-main"
  },
  "require": {
    "aws/aws-sdk-php": "^3.270",
    "setasign/fpdf": "^1.8",
    "setasign/fpdi": "^2.3",
    "mailchimp/marketing": "^3.0",
    "mailchimp/transactional": "^1.0"
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "coverage": "mkdir -p /var/www/html/coverage && phpunit --colors=always --coverage-html /var/www/html/coverage"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "extra": {
    "installer-paths": {
      "vendor/{$vendor}/{$name}": [
        "wpackagist-plugin/woocommerce"
      ]
    }
  }
}
