{
  "name": "wstrategies/wp-bracket-builder",
  "authors": [
    {
      "name": "Barry Molina",
      "email": "barry@wstrategies.co"
    }
  ],
  "autoload": {
    "psr-4": {
      "WStrategies\\BMB\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WStrategies\\BMB\\": ""
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*"
      ]
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "yoast/phpunit-polyfills": "^1.0",
    "spatie/phpunit-snapshot-assertions": "^4.2",
    "wpackagist-plugin/woocommerce": "8.4.0",
    "php-stubs/wordpress-tests-stubs": "^6.3",
    "phpstan/phpstan": "^1.10",
    "php-stubs/wordpress-stubs": "^6.4",
    "sentry/sentry": "^4.3",
    "php-stubs/woocommerce-stubs": "^8.4",
    "10up/wp_mock": "^1.0"
  },
  "require": {
    "aws/aws-sdk-php": "^3.270",
    "setasign/fpdf": "^1.8",
    "setasign/fpdi": "^2.3",
    "mailchimp/marketing": "^3.0",
    "mailchimp/transactional": "^1.0"
  },
  "scripts": {
    "test-integration": "phpunit --colors=always --bootstrap=integration-tests/bootstrap.php --configuration=integration-tests/phpunit.xml.dist",
    "test-unit": "phpunit --colors=always --bootstrap=unit-tests/bootstrap.php --configuration=unit-tests/phpunit.xml.dist",
    "coverage": "mkdir -p /var/www/html/coverage && phpunit --colors=always --coverage-html /var/www/html/coverage"
  },
  "extra": {
    "installer-paths": {
      "vendor/{$vendor}/{$name}": [
        "wpackagist-plugin/woocommerce"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
