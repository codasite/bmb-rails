includes:
  - phpstan-baseline.neon
parameters:
  excludePaths:
    - vendor/*
    - integration-tests/*
    - unit-tests/*
  level: 8
  scanFiles:
    - vendor/php-stubs/wordpress-stubs/wordpress-stubs.php
    - vendor/php-stubs/wordpress-tests-stubs/wordpress-tests-stubs.php
    - vendor/php-stubs/woocommerce-stubs/woocommerce-stubs.php
  bootstrapFiles:
    - constants.php
